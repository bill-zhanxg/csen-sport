		"playwright": "^1.46.0",
		"postcss": "^8.4.41",
		"tailwindcss": "^3.4.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.4",
		"vitest": "^2.0.5"
	},
	"engines": {
		"node": ">=20.x.x"
	}
}
